@Article{LandbergShao,
author="Lansberg, Jean-Philippe
and Shao, Hua-Sheng",
title="Towards an automated tool to evaluate the impact of the nuclear modification of the gluon density on quarkonium, D and B meson production in proton--nucleus collisions",
journal="The European Physical Journal C",
year="2016",
month="Dec",
day="27",
volume="77",
number="1",
pages="1",
abstract="We propose a simple and model-independent procedure to account for the impact of the nuclear modification of the gluon density as encoded in nuclear collinear PDF sets on two-to-two partonic hard processes in proton--nucleus collisions. This applies to a good approximation to quarkonium, D and B meson production, generically referred to {\$}{\$}{\backslash}mathcal {\{}H{\}}{\$}{\$}H. Our procedure consists in parametrising the square of the parton scattering amplitude, {\$}{\$}{\{}{\backslash}mathcal {\{}A{\}}{\}}{\_}{\{}gg {\backslash}rightarrow {\{}{\backslash}mathcal {\{}H{\}}{\}} X{\}}{\$}{\$}Agg{\textrightarrow}HXand constraining it from the proton--proton data. Doing so, we have been able to compute the corresponding nuclear modification factors for {\$}{\$}J/{\backslash}psi {\$}{\$}J/$\psi$, {\$}{\$}{\backslash}Upsilon {\$}{\$}$\Upsilon$and {\$}{\$}D^0{\$}{\$}D0as a function of y and {\$}{\$}P{\_}T{\$}{\$}PTat {\$}{\$}{\backslash}sqrt{\{}s{\_}{\backslash}mathrm{\{}NN{\}}{\}}=5{\$}{\$}sNN=5and 8 TeV in the kinematics of the various LHC experiments in a model independent way. It is of course justified since the most important ingredient in such evaluations is the probability of each kinematical configuration. Our computations for D mesons can also be extended to B meson production. To further illustrate the potentiality of the tool, we provide -- for the first time -- predictions for the nuclear modification factor for {\$}{\$}{\backslash}eta {\_}c{\$}{\$}$\eta$cproduction in {\$}{\$}p{\$}{\$}pPb collisions at the LHC.",
issn="1434-6052",
doi="10.1140/epjc/s10052-016-4575-x",
url="https://doi.org/10.1140/epjc/s10052-016-4575-x"
}

@Article{fragmentationAA,
author="Plumari, S.
and Minissale, V.
and Das, S. K.
and Coci, G.
and Greco, V.",
title="Charmed hadrons from coalescence plus fragmentation in relativistic nucleus-nucleus collisions at RHIC and LHC",
journal="The European Physical Journal C",
year="2018",
month="Apr",
day="28",
volume="78",
number="4",
pages="348",
abstract="In a coalescence plus fragmentation approach we calculate the heavy baryon/meson ratio and the {\$}{\$}p{\_}T{\$}{\$}pTspectra of charmed hadrons {\$}{\$}D^{\{}0{\}}{\$}{\$}D0, {\$}{\$}D{\_}{\{}s{\}}{\$}{\$}Dsand {\$}{\$}{\backslash}varLambda {\_}{\{}c{\}}^{\{}+{\}}{\$}{\$}$\Lambda$c+in a wide range of transverse momentum from low {\$}{\$}p{\_}T{\$}{\$}pTup to about 10 GeV and discuss their ratios from RHIC to LHC energies without any change of the coalescence parameters. We have included the contribution from decays of heavy hadron resonances and also the one due to fragmentation of heavy quarks which do not undergo the coalescence process. The coalescence process is tuned to have all charm quarks hadronizing in the {\$}{\$}p{\_}T{\backslash}rightarrow 0{\$}{\$}pT{\textrightarrow}0limit and at finite {\$}{\$}p{\_}T{\$}{\$}pTcharm quarks not undergoing coalescence are hadronized by independent fragmentation. The {\$}{\$}p{\_}T{\$}{\$}pTdependence of the baryon/meson ratios are found to be sensitive to the masses of coalescing quarks, in particular the {\$}{\$}{\backslash}varLambda {\_}{\{}c{\}}/D^{\{}0{\}}{\$}{\$}$\Lambda$c/D0can reach values of about {\$}{\$}{\backslash}mathrm 1{\backslash}div 1.5 {\$}{\$}1{\textdiv}1.5at {\$}{\$}p{\_}T {\backslash}approx {\backslash}, 3{\$}{\$}pT≈3GeV, or larger, similarly to the light baryon/meson ratio like {\$}{\$}p/{\backslash}pi {\$}{\$}p/$\pi$and {\$}{\$}{\backslash}varLambda /K{\$}{\$}$\Lambda$/K, however a marked difference is a quite weak {\$}{\$}p{\_}T{\$}{\$}pTdependence with respect to the light case, such that a larger value at intermediate {\$}{\$}p{\_}T{\$}{\$}pTimplies a relatively large value also for the integrated yields. A comparison with other coalescence model and with the prediction of thermal model is discussed.",
issn="1434-6052",
doi="10.1140/epjc/s10052-018-5828-7",
url="https://doi.org/10.1140/epjc/s10052-018-5828-7"
}

