@Article{ShaoSongPP,
author="Song, Jun
and Li, Hai-hong
and Shao, Feng-lan",
title="New feature of low {$p_\text{T}$} charm quark hadronization in pp collisions at {$\sqrt{s}=7$} TeV",
journal="The European Physical Journal C",
year="2018",
month="Apr",
day="28",
volume="78",
number="4",
pages="344",
abstract="Treating the light-flavor constituent quarks and antiquarks whose momentum information is extracted from the data of soft light-flavor hadrons in pp collisions at {\$}{\$}{\backslash}sqrt{\{}s{\}}=7{\$}{\$}s=7TeV as the underlying source of chromatically neutralizing the charm quarks of low transverse momenta ({\$}{\$}p{\_}{\{}T{\}}{\$}{\$}pT), we show that the experimental data of {\$}{\$}p{\_}{\{}T{\}}{\$}{\$}pTspectra of single-charm hadrons {\$}{\$}D^{\{}0,+{\}}{\$}{\$}D0,+, {\$}{\$}D^{\{}*+{\}}{\$}{\$}D∗+{\$}{\$}D{\_}{\{}s{\}}^{\{}+{\}}{\$}{\$}Ds+, {\$}{\$}{\backslash}varLambda {\_}{\{}c{\}}^{\{}+{\}}{\$}{\$}$\Lambda$c+and {\$}{\$}{\backslash}varXi {\_}{\{}c{\}}^{\{}0{\}}{\$}{\$}$\Xi$c0at mid-rapidity in the low {\$}{\$}p{\_}{\{}T{\}}{\$}{\$}pTrange ({\$}{\$}2{\backslash}lesssim p{\_}{\{}T{\}}{\backslash}lesssim 7{\$}{\$}2≲pT≲7GeV/c) in pp collisions at {\$}{\$}{\backslash}sqrt{\{}s{\}}=7{\$}{\$}s=7TeV can be well understood by the equal-velocity combination of perturbatively created charm quarks and those light-flavor constituent quarks and antiquarks. This suggests a possible new scenario of low {\$}{\$}p{\_}{\{}T{\}}{\$}{\$}pTcharm quark hadronization, in contrast to the traditional fragmentation mechanism, in pp collisions at LHC energies. This is also another support for the exhibition of the soft constituent quark degrees of freedom for the small parton system created in pp collisions at LHC energies.",
issn="1434-6052",
doi="10.1140/epjc/s10052-018-5817-x",
url="https://doi.org/10.1140/epjc/s10052-018-5817-x"
}

@Article{ZEUSep,
author="{The ZEUS Collaboration}",
title="Measurement of charm fragmentation ratios and fractionsin photoproduction at HERA",
journal="The European Physical Journal C - Particles and Fields",
year="2005",
month="Nov",
day="01",
volume="44",
number="3",
pages="351--366",
abstract="The production of {\$}D^{\{}{\backslash}ast + {\}}{\$}, D0, D + , Ds+  and {\$}{\backslash}Lambda{\_}c^ + {\$}charm hadrons and their antiparticles in e p scattering at HERA was measured with the ZEUS detector using an integrated luminosity of {\$}79 {\backslash}rm {\{}pb{\}}^{\{}-1{\}}{\$}. The measurement has been performed in the photoproduction regime with the exchanged-photon virtuality {\$}Q^2 < 1 {\backslash}rm {\{}GeV{\}}^2{\$}and for photon-proton centre-of-mass energies in the range {\$}130 < W < 300 {\backslash}rm {\{}GeV{\}}{\$}. The charm hadrons were reconstructed in the range of transverse momentum {\$}p{\_}T(D,{\backslash}Lambda{\_}c) > 3.8 {\backslash}rm {\{}GeV{\}}{\$}and pseudorapidity {\$}{\backslash}vert{\backslash}eta(D,{\backslash}Lambda{\_}c){\backslash}vert < 1.6{\$}. The production cross sections were used to determine the ratio of neutral and charged D-meson production rates, {\$}{\backslash}smash{\{}R{\_}{\{}u/d{\}}{\}}{\$}, the strangeness-suppression factor, {\$}{\backslash}gamma{\_}s{\$}, and the fraction of charged D mesons produced in a vector state, {\$}{\backslash}smash{\{}P^d{\_}{\{}{\backslash}rm v{\}}{\}}{\$}. The measured {\$}{\backslash}smash{\{}R{\_}{\{}u/d{\}}{\}}{\$}and {\$}{\backslash}smash{\{}{\backslash}gamma{\_}s{\}}{\$}values agree with those obtained in deep inelastic scattering and in e + e- annihilations. The measured {\$}{\backslash}smash{\{}P^d{\_}{\{}{\backslash}rm v{\}}{\}}{\$}value is smaller than, but consistent with, the previous measurements. The fractions of c quarks hadronising as a particular charm hadron, {\$}f(c {\backslash}rightarrow D, {\backslash}Lambda{\_}c){\$}, were derived in the given kinematic range. The measured open-charm fragmentation fractions are consistent with previous results, although the measured {\$}f(c{\backslash}rightarrow D^{\{}* + {\}}){\$}is smaller and {\$}f(c{\backslash}rightarrow {\backslash}Lambda{\_}c^ + ){\$}is larger than those obtained in e + e- annihilations. These results generally support the hypothesis that fragmentation proceeds independently of the hard sub-process.",
issn="1434-6052",
doi="10.1140/epjc/s2005-02397-3",
url="https://doi.org/10.1140/epjc/s2005-02397-3"
}

