@article{pp500GeVcharm,
  author={David Tlustý and the Star collaboration},
  title={Open charm measurements in p + p collisions at STAR},
  journal={Journal of Physics: Conference Series},
  volume={509},
  number={1},
  pages={012078},
  url={http://stacks.iop.org/1742-6596/509/i=1/a=012078},
  year={2014},
  abstract={In this article, we will present the STAR results of open charm hadron and non-photonic productions at mid-rapidity in p+p collisions at ##IMG## [http://ej.iop.org/images/1742-6596/509/1/012078/jpconf14_509_012078_eqn001.gif] = 200 and 500 GeV. Open charm mesons are reconstructed directly via hadronic decay channels with daughter particles identified by STAR Time Projection Chamber (TPC) and Time Of Flight (TOF) detectors. Non-photonic electron yields are calculated by subtracting photonic electrons from inclusive electrons identified using TPC and Electromagnetic Calorimeter. These measurements are compared to theoretical model calculations and physics implications will be discussed.}
}
	
